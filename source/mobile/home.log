        StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/

// set content type for wireless data
response.setContentType("text/vnd.wap.wml");

// write the data
mb.append("<?xml version=\"1.0\"?>");
mb.append("<!DOCTYPE wml PUBLIC \"-//WAPFORUM//DTD WML 1.1//EN\"");
mb.append(" \"http://www.wapforum.org/DTD/wml_1.1.xml\">");
mb.append("<wml>");
mb.append("<card title=\"Home\">");

mb.append("<p>");
mb.append("<a href=\"recententries.log\">Recent Entries</a>");
mb.append("</p>");

if (userSession.getAccountuser().isLoggedIn){
    mb.append("<p>");
    mb.append("<a href=\"trafficsummary.log\">Traffic Summary</a>");
    mb.append("</p>");

    mb.append("<p>");
    mb.append("<a href=\"traffictopten.log\">Top Ten Entries</a>");
    mb.append("</p>");

    mb.append("<p>");
    mb.append("<a href=\"trafficbylog.log\">By Log</a>");
    mb.append("</p>");

    mb.append("<p>");
    mb.append("<a href=\"chooselog.log\">Add Entry</a>");
    mb.append("</p>");

    mb.append("<p>");
    mb.append("<a href=\"logoff.log\">Log Off</a>");
    mb.append("</p>");
} else {
    mb.append("<p>");
    mb.append("<a href=\"mobilelogin.log\">Log On</a>");
    mb.append("</p>");
}





mb.append("</card>");
mb.append("</wml>");

%>

