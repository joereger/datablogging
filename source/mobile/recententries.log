<%@ page import="reger.core.db.Db"%>
<%
/*----------------------------------------------------*/
/*                  Page Config                       */
reger.pageFramework.PageProps pageProps = new reger.pageFramework.PageProps();
pageProps.siteSection=pageProps.MOBILEPUBLIC;
pageProps.isPasswordProtected = false;
pageProps.aclObjectName = "MOBILE";
pageProps.trafficType=reger.Vars.TRAFFICTYPEPUBLICHOMEPAGE;
pageProps.pathToAppRoot="../";
/*----------------------------------------------------*/
%>

<%@ include file="../globalheader.jsp" %>

<%
/*----------------------------------------------------*/
/*                  Main Body                         */
        StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/

// set content type for wireless data
response.setContentType("text/vnd.wap.wml");

// write the data
mb.append("<?xml version=\"1.0\"?>");
mb.append("<!DOCTYPE wml PUBLIC \"-//WAPFORUM//DTD WML 1.1//EN\"");
mb.append(" \"http://www.wapforum.org/DTD/wml_1.1.xml\">");
mb.append("<wml>");
mb.append("<card title=\"Recent Entries\">");

//-----------------------------------
//-----------------------------------
String[][] rstEventList= Db.RunSQL("SELECT eventid, title FROM event, megalog WHERE event.accountid='"+ userSession.getAccount().getAccountid() +"' AND event.logid=megalog.logid AND "+userSession.getAccountuser().LogsUserCanViewQueryend(userSession.getAccount().getAccountid())+" ORDER BY date DESC LIMIT 0,15");
//-----------------------------------
//-----------------------------------
if (rstEventList!=null && rstEventList.length>0){
	for(int i=0; i<rstEventList.length; i++){
	    mb.append("<p>");
		mb.append("<a href=\"entryview.log?eventid="+ rstEventList[i][0] +"\">" + reger.core.Util.truncateString(rstEventList[i][1], 50) + "</a>");
	    mb.append("</p>");
	}
} else {
    mb.append("<p>");
    mb.append("No Entries Found.");
    mb.append("</p>");
}


mb.append("</card>");
mb.append("</wml>");

%>

