<%
/*----------------------------------------------------*/
/*                  Page Config                       */
reger.pageFramework.PageProps pageProps = new reger.pageFramework.PageProps();
pageProps.siteSection=pageProps.MASTERADMINSITE;
pageProps.title = "Cache";
pageProps.isPasswordProtected = true;
pageProps.navButtonName = "loecache";
pageProps.aclObjectName = "MASTERADMIN";
pageProps.pathToAppRoot="../";
/*----------------------------------------------------*/
%>

<%@ include file="../globalheader.jsp" %>

<%
/*----------------------------------------------------*/
/*                  Main Body                         */
    StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/

int levelsToDisplay = 1;
if (request.getParameter("levelstodisplay")!=null && reger.core.Util.isinteger(request.getParameter("levelstodisplay"))){
    levelsToDisplay = Integer.parseInt(request.getParameter("levelstodisplay"));
}

String fqn = "/";
if (request.getParameter("fqn")!=null && !request.getParameter("fqn").equals("")){
    fqn = request.getParameter("fqn");
}

//Display the fqn
pageProps.title = "Cache: " + fqn;

//Output the cache as html
mb.append(reger.cache.jboss.CacheDumper.getHtml(fqn, levelsToDisplay));

%>

