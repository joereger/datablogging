%>

<%@ include file="../globalheader.jsp" %>

<%
/*----------------------------------------------------*/
/*                  Main Body                         */
    StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/



mb.append("<br><br>");

String cellbgcolor="#cccccc";

mb.append("<table cellpadding=3 cellspacing=1 width=100% border=0 bgcolor=#ffffff>");

//New log type row
mb.append("<tr><td valign=top align=left bgcolor=#ffffff nowrap colspan=2><br><a href='logtypes-type.log?action=startadd'><img src='../images/plus-icon.gif' width='15' height='15' alt='' border='0'><font face=arial size=-1><b>Create a New Log Type</b></a></td></tr>");

//Header
mb.append("<tr><td valign=top align=left bgcolor=#666666 colspan=4><font face=arial size=+1 color=#ffffff><b>Log Types</b></font>");
mb.append("<br>");
mb.append("<font face=arial size=-1 color=#ffffff>");
mb.append("This is a list of log types in the system.  Note that none of these log types will appear to users until the private label/log type relationship is made.");
mb.append("</font>");
mb.append("</tr>");

//Body
mb.append("<tr><td valign=top align=left bgcolor="+ cellbgcolor +" nowrap><font face=arial size=-2>Log Type</font></td><td valign=center align=left bgcolor="+ cellbgcolor +"><font face=arial size=-2>Description</font></td></tr>");
//-----------------------------------
//-----------------------------------
String[][] rstLogtype= Db.RunSQL("SELECT eventtypeid, megalogname, description FROM megalogtype WHERE issystemlogtype='1'");
//-----------------------------------
//-----------------------------------
if (rstLogtype!=null){
    for(int i=0; i<rstLogtype.length; i++){

        //Flip flop colors
        if (cellbgcolor.equals("#ffffff")) {
            cellbgcolor="#ffffff";
        } else {
            cellbgcolor="#ffffff";
        }

        //Each log type output
        mb.append("<tr><td valign=top align=left bgcolor="+ cellbgcolor +" nowrap><a href='logtypes-type.log?eventtypeid="+ rstLogtype[i][0] +"'><img src='../images/round-arrow-green.gif' width='15' height='15' alt='' border='0'><font face=arial size=-1><b>"+ reger.core.Util.cleanForHtml(rstLogtype[i][1]) +"</b></a></td><td valign=center align=left bgcolor="+ cellbgcolor +"><font face=arial size=-2 color=#000000>"+ reger.core.Util.cleanForHtml(rstLogtype[i][2]) +"</td></tr>");


    }
} else {
    mb.append("<tr><td valign=top align=left bgcolor="+ cellbgcolor +" nowrap><img src='../images/info-icon.gif' alt='' border='0'><font face=arial size=-1>This system has no logs.  This is a problem.</td><td valign=center align=left bgcolor="+ cellbgcolor +"><font face=arial size=-2>&nbsp;</td></tr>");
}


mb.append("</table>");
mb.append("<br><br>");




%>

