
<%@ page import="java.util.*,
                 reger.core.bandwidthtest.BandwidthTest" %>

<%
/*----------------------------------------------------*/
/*                  Page Config                       */
reger.pageFramework.PageProps pageProps = new reger.pageFramework.PageProps();
pageProps.siteSection=pageProps.MASTERADMINSITE;
pageProps.title = "Bandwidth Connectivity and Speed";
pageProps.isPasswordProtected = true;
pageProps.navButtonName = "loebandwidthspeed";
pageProps.aclObjectName = "MASTERADMIN";
pageProps.pathToAppRoot="../";
/*----------------------------------------------------*/
%>

<%@ include file="../globalheader.jsp" %>

<%
/*----------------------------------------------------*/
/*                  Main Body                         */
    StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/


mb.append("<table cellpadding=5 cellspacing=2 border=0>");
mb.append("<tr>");
mb.append("<td valign=top bgcolor=#e6e6e6>");
mb.append("<font face=arial size=-1>");
mb.append("Successful Test?");
mb.append("</font>");
mb.append("</td>");
mb.append("<td valign=top bgcolor=#e6e6e6>");
mb.append("<font face=arial size=-1>");
mb.append("Time Test Done");
mb.append("</font>");
mb.append("</td>");
mb.append("<td valign=top bgcolor=#e6e6e6>");
mb.append("<font face=arial size=-1>");
mb.append("Speed in kbps");
mb.append("</font>");
mb.append("</td>");
mb.append("</tr>");

Vector results = reger.core.bandwidthtest.BandwidthTest.getAllTestResults();
if (results!=null && results.size()>0){
    for (int i = (results.size()-1); i >= 0; i--) {
        BandwidthTest bwtest = (BandwidthTest) results.get(i);
        mb.append("<tr>");
        mb.append("<td valign=top>");
        mb.append("<font face=arial size=-2>");
        mb.append(bwtest.wasSuccessfulTest);
        mb.append("</font>");
        mb.append("</td>");
        mb.append("<td valign=top>");
        mb.append("<font face=arial size=-2>");
        mb.append(reger.core.TimeUtils.dateformatfordb(bwtest.timeTestDone));
        mb.append("</font>");
        mb.append("</td>");
        mb.append("<td valign=top>");
        mb.append("<font face=arial size=-2>");
        mb.append(bwtest.speedInKbps);
        mb.append("</font>");
        mb.append("</td>");
        mb.append("</tr>");
    }
} else {
    mb.append("<tr>");
    mb.append("<td valign=top colspan=3>");
    mb.append("<font face=arial size=-2>");
    mb.append("No tests have been done since the blogging server was started.");
    mb.append("</font>");
    mb.append("</td>");
    mb.append("</tr>");
}

mb.append("</table>");

mb.append("<br>");



%>

