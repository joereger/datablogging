reger.demos.DemoCategory[] demoCategories = reger.demos.AllDemos.getAllDemoCategories();
    for (int j = 0; j < demoCategories.length; j++) {

        mb.append("<center>");
        mb.append("<br><br>");
        mb.append("<font face=arial size=+3 color=#cccccc><b>");
        mb.append(demoCategories[j].getCategoryName());
        mb.append("</b></font>");
        mb.append("</center>");
        mb.append("<br>");

        reger.demos.Demo[] demos = demoCategories[j].getDemos();
        if (demos.length>0){
            int numofcols = 3;
            int currentcol = 0;
            int width = 100/numofcols;
            mb.append("<center>");
            mb.append("<table cellpadding=3 width=85% cellspacing=1 border=0>");

            for (int i = 0; i < demos.length; i++) {
                Demo demo = demos[i];
                currentcol = currentcol + 1;
                if (currentcol==1){
                    mb.append("<tr>");
                }
                mb.append("<td valign=top width="+width+"% >");
                mb.append(reger.demos.DemoRenderHtml.getSummary(demo, pageProps.pathToAppRoot));
                mb.append("</td>");
                if (currentcol==numofcols){
                    mb.append("</tr>");
                    currentcol=0;
                }
            }
            if (currentcol<numofcols){
                for (int i = 0; i < (numofcols-currentcol); i++) {
                    mb.append("<td valign=top width="+width+"% >");
                    mb.append("&nbsp;");
                    mb.append("</td>");
                }
                mb.append("</tr>");
            }

            mb.append("</table>");
            mb.append("</center>");
        }

    }




