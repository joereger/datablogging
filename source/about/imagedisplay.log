        StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/

//The goal of this page is simply to accept a filename via url line and output an image tag with that filename
mb.append("<html><head><title></title></head><body bgcolor=#ffffff link='#0000ff' vlink='#0000ff' text='#000000' LEFTMARGIN='0' TOPMARGIN='0' MARGINWIDTH='0' MARGINHEIGHT='0'>");

if (request.getParameter("imagename")!=null && !request.getParameter("imagename").equals("")){

    //Output the image tag
    mb.append("<img src='images/logscreenshots/");
    mb.append(request.getParameter("imagename"));
    mb.append("' border=0>");

} else {
    mb.append("Image not found.");
    reger.core.Util.debug(5, "imagedisplay.log called with no imagename.");
}

mb.append("</body></html>");

//Output to the client
out.print(mb.toString());


%>

