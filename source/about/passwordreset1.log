<%@ page import="reger.core.db.Db"%>
<%
/*----------------------------------------------------*/
/*                  Page Config                       */
reger.pageFramework.PageProps pageProps = new reger.pageFramework.PageProps();
pageProps.siteSection=pageProps.MARKETINGSITE;
pageProps.isPasswordProtected = false;
pageProps.trafficType=reger.Vars.TRAFFICTYPEMARKETINGSITEMISC;
pageProps.pathToAppRoot="../";
pageProps.marketingSiteSection = pageProps.MARKETINGSITESECTIONHOME;
/*----------------------------------------------------*/
%>

<%@ include file="../globalheader.jsp" %>






<%
/*----------------------------------------------------*/
/*                  Main Body                         */
        StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/

if(request.getParameter("msg")!=null && request.getParameter("msg").equals("usernamenotfound")){
    mb.append("<blockquote>");
    mb.append(reger.InfoBox.get(reger.InfoBox.BOXTYPEERROR, pageProps.pathToAppRoot, "That username was not found.  Please try again."));
    mb.append("</blockquote>");
}

mb.append("<br><br>");
mb.append("<blockquote>");
mb.append("<font face=arial size=+1>");
mb.append("To Reset Your Password, Please Enter Your Username:");
mb.append("</font>");
mb.append("<br>");
mb.append("<br>");
mb.append("<form action=passwordreset2.log>");
mb.append("<input type=hidden name=action value=continue>");
//Begin login table
mb.append("<table cellpadding=10 cellspacing=5 border=0>");
mb.append("<tr>");
mb.append("<td valign=middle align=right>");
mb.append("<font class=mediumfont face=arial size=-1>");
mb.append("<strong>Username:</strong>");
mb.append("</font>");
mb.append("</td>");
mb.append("<td valign=top align=left>");
mb.append("<input type='text' name='username' size=10 maxlength=50 style=\"font-size: 18px;\" value=\"\">");
mb.append("</td>");
mb.append("<tr>");
mb.append("<tr>");
mb.append("<td valign=top align=left>");
mb.append("<font class=mediumfont face=arial size=-1>");
mb.append("&nbsp;");
mb.append("</font>");
mb.append("</td>");
mb.append("<td valign=top align=left>");
mb.append("<input type='submit' value='Continue'>");
mb.append("</form>");
mb.append("</td>");
mb.append("</tr>");
mb.append("</table>");
//End login table
mb.append("</blockquote>");



%>

