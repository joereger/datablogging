<%@ include file="../globalheader.jsp" %>

<%
/*----------------------------------------------------*/
/*                  Main Body                         */
        StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/


mb.append("<p align=right>");
mb.append("<font face=arial size=-2>");
mb.append("<a href=\"javascript:window.close();\">");
mb.append("Close This Window");
mb.append("</a>");
mb.append("</font>");
mb.append("</p>");

//mb.append("<font face=arial size=+1>");
//mb.append("<b>");
//mb.append("Help");
//mb.append("</b>");
//mb.append("</font>");

mb.append("<br>");

//-----------------------------------
//-----------------------------------
String[][] rstHelp= db.RunSQL("SELECT title, body FROM help WHERE helpkey='"+request.getParameter("helpkey")+"'");
//-----------------------------------
//-----------------------------------
if (rstHelp!=null && rstHelp.length>0){
    mb.append("<font face=arial size=-1>");
    mb.append("<b>");
    mb.append(rstHelp[0][0]);
    mb.append("</b>");
    mb.append("<br><br>");
    mb.append(rstHelp[0][1]);
    mb.append("</font>");
} else {
    mb.append("<font face=arial size=-1>");
    mb.append("No help is available for this item at this time.");
    mb.append("</font>");
}



%>

