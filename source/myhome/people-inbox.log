/*----------------------------------------------------*/
%>

<%@ include file="../globalheader.jsp" %>

<%

/*----------------------------------------------------*/
/*                  Main Body                         */
        StringBuffer mb = new StringBuffer();
/*----------------------------------------------------*/


//Delete
if (pageProps.action.equals("delete")){
    //-----------------------------------
    //-----------------------------------
    int count = Db.RunSQLUpdate("DELETE FROM friendmessagerecipient WHERE accountuserid='"+userSession.getAccountuser().getAccountuserid()+"' AND friendmessagerecipientid='"+request.getParameter("friendmessagerecipientid")+"'");
    //-----------------------------------
    //-----------------------------------
}

//Mark Unread
if (pageProps.action.equals("markunread")){
    //-----------------------------------
    //-----------------------------------
    int count = Db.RunSQLUpdate("UPDATE friendmessagerecipient SET isread='0' WHERE accountuserid='"+userSession.getAccountuser().getAccountuserid()+"' AND friendmessagerecipientid='"+request.getParameter("friendmessagerecipientid")+"'");
    //-----------------------------------
    //-----------------------------------
}

//Mark Read
if (pageProps.action.equals("markread")){
    //-----------------------------------
    //-----------------------------------
    int count = Db.RunSQLUpdate("UPDATE friendmessagerecipient SET isread='1' WHERE accountuserid='"+userSession.getAccountuser().getAccountuserid()+"' AND friendmessagerecipientid='"+request.getParameter("friendmessagerecipientid")+"'");
    //-----------------------------------
    //-----------------------------------
}

//Put up the main inbox
mb.append(reger.InboxHtml.getInboxListing(true, -1, pageProps, userSession, request));

%>

