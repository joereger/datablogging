<%

if (request.getParameter("accountid")!=null && reger.core.Util.isinteger(request.getParameter("accountid"))){
    reger.Account acctTmp = reger.cache.AccountCache.get(Integer.parseInt(request.getParameter("accountid")));
    try {
        response.sendRedirect(acctTmp.getSiteRootUrl(userSession) + "/");
        return;
    } catch (Exception e){
        reger.util.errorsave(e);
    }
    out.print("Please click <a href='" + acctTmp.getSiteRootUrl(userSession) + "/'>here</a>.");
} else {
    out.print("Site not found.  Please try again.");    
}

%>